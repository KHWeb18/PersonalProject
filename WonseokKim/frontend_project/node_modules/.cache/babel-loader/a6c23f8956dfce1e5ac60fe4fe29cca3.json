n        }\n    },\n    created () {\n        this.fetchQuestion(this.questionNo)\n                .catch(err => {\n                    alert(err.response.data.message)\n                    this.$router.back()\n                })\n    }\n}\n</script>"],"sourceRoot":"src/views"}]}